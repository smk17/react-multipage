  "scripts": {
    "index": "cross-env PORT=8081 APP=IndexCustomize node scripts/start.js",
    "contract": "cross-env PORT=3000 APP=ContractManagement node scripts/start.js",
    "middleground": "cross-env PORT=8888 APP=Middleground node scripts/start.js",
    "demo": "cross-env PORT=3000 APP=demo node scripts/start.js",
    "build-index": "cross-env PORT=8081 APP=IndexCustomize node scripts/build.js",
    "build-contract": "cross-env PORT=3000 APP=ContractManagement node scripts/build.js",
    "build-middleground": "cross-env PORT=8888 APP=Middleground node scripts/build.js",
    "build-demo": "cross-env PORT=3000 APP=demo node scripts/build.js",
    "start": "node scripts/start.js",
    "serve": "cross-env HTTPS=true yarn start",
    "build": "node scripts/build.js",
    "dev": "serve -s build",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "autoprefixer": "^8.6.5",
    "babel-core": "6.26.0",
